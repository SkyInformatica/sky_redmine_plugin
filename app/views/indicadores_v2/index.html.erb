<% content_for :header_tags do %>
  <%= stylesheet_link_tag "indicadores_v2", plugin: "sky_redmine_plugin" %>
<% end %>

<div class="indicadores-v2-container">  
  <div class="metabase-dashboard">
    <div class="metabase-link">
      <%= link_to "Abrir no Metabase", @metabase_direct_url, target: "_blank" %>
    </div>
    <iframe 
      src="<%= @iframe_url %>" 
      frameborder="0" 
      width="100%" 
      height="1080px" 
      allowtransparency>
    </iframe>
  </div>
</div>
