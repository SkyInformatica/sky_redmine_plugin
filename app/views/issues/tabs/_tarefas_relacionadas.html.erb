<% # plugins/sky_redmine_plugin/app/views/issues/tabs/_tarefas_relacionadas.html.erb %>  

<strong>Antes do partial</strong>  

<% # Verificar se @issue está definido %>  
<% unless @issue %>  
  <% Rails.logger.info "DEBUG: @issue não está definido no contexto" %>  
<% else %>  
  <% Rails.logger.info "DEBUG: @issue está definido: #{@issue.inspect}" %>  
<% end %>  

<% # Definir @project %>  
<% @project ||= @issue.project %>  

<% Rails.logger.info "DEBUG: @project: #{@project.inspect}" %>  

<% # Definir @relations %>  
<% @relations = @issue.relations.select { |r| r.other_issue(@issue).visible? } %>  

<% Rails.logger.info "DEBUG: @relations: #{@relations.inspect}" %>  

<%= render partial: "relations" %>  

<strong>Após o partial</strong>