<% if @issue.status.name == "Teste NOK" && ["Notarial - QS", "Registral - QS"].include?(@issue.project.name) %>
  <li><%= link_to "Criar tarefa de retorno de testes", criar_retorno_testes_qs_path(id: @issue.id), class: "context-menu-item" %></li>
<% elsif @issue.status.name == "Resolvida" && !["Notarial - QS", "Registral - QS"].include?(@issue.project.name) %>
  <li><%= link_to "Criar tarefa de retorno de testes para desenvolvimento", criar_retorno_testes_devel_path(id: @issue.id), class: "context-menu-item" %></li>
<% end %>